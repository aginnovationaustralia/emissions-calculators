{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "bundle": {
      "outputs": ["bundle/**"],
      "dependsOn": ["build"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {},
    "check:types": {},
    "clean": {
      "cache": false
    }
  }
}
