{
  "name": "@aginnovationaustralia/emissions-calculators",
  "version": "0.1.4",
  "type": "module",
  "main": "dist/versions/3.0.0/index.cjs",
  "module": "dist/versions/3.0.0/index.mjs",
  "types": "dist/versions/3.0.0/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/versions/3.0.0/index.d.ts",
      "import": "./dist/versions/3.0.0/index.mjs",
      "require": "./dist/versions/3.0.0/index.cjs"
    },
    "./versions/3.0.0": {
      "types": "./dist/versions/3.0.0/index.d.ts",
      "import": "./dist/versions/3.0.0/index.mjs",
      "require": "./dist/versions/3.0.0/index.cjs"
    },
    "./versions/3.0.0/*": {
      "types": "./dist/versions/3.0.0/*",
      "import": "./dist/versions/3.0.0/*",
      "require": "./dist/versions/3.0.0/*"
    },
    "./versions/*": {
      "types": "./dist/versions/*",
      "import": "./dist/versions/*",
      "require": "./dist/versions/*"
    },
    "./*": {
      "types": "./dist/versions/3.0.0/*",
      "import": "./dist/versions/3.0.0/*",
      "require": "./dist/versions/3.0.0/*"
    },
    "./calculators": {
      "types": "./dist/versions/3.0.0/calculators.d.ts",
      "import": "./dist/versions/3.0.0/calculators.mjs",
      "require": "./dist/versions/3.0.0/calculators.cjs"
    }
    
  },
  "typesVersions": {
    "*": {
      "versions/*": [
        "dist/versions/*"
      ],
      "*": [
        "dist/versions/3.0.0/*"
      ]
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "check:types": "tsc --noEmit",
    "lint": "pnpm run check:types",
    "test": "jest",
    "version": "npm version --no-git-tag-version",
    "build:docs": "tsx scripts/build-docs/index.ts"
  },
  "author": "Agricultural Innovation Australia",
  "license": "ISC",
  "description": "",
  "engines": {
    "node": ">=22.19.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@typescript-eslint/parser": "^8.44.0",
    "jest": "^30.1.3",
    "nanoclone": "^1.0.2",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "class-validator-jsonschema": "^5.1.0",
    "mixpanel": "^0.18.1",
    "semver": "^7.7.2",
    "ts-deepmerge": "^7.0.3",
    "type-fest": "^4.41.0"
  }
}
