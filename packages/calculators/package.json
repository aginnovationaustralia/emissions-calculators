{
  "name": "@aginnovationaustralia/emissions-calculators",
  "version": "0.1.2",
  "main": "dist/latest.js",
  "types": "dist/latest.d.ts",
  "exports": {
    ".": {
      "types": "./dist/latest.d.ts",
      "import": "./dist/latest.js",
      "require": "./dist/latest.js"
    },
    "./versions/*": {
      "types": "./dist/versions/*",
      "import": "./dist/versions/*",
      "require": "./dist/versions/*"
    },
    "./*": {
      "types": "./dist/versions/3.0.0/*",
      "import": "./dist/versions/3.0.0/*",
      "require": "./dist/versions/3.0.0/*"
    },
    "./calculators": {
      "types": "./dist/versions/3.0.0/calculators.d.ts",
      "import": "./dist/versions/3.0.0/calculators.mjs",
      "require": "./dist/versions/3.0.0/calculators.js"
    },
    "./versions/3.0.0/calculators": {
      "types": "./dist/versions/3.0.0/calculators.d.ts",
      "import": "./dist/versions/3.0.0/calculators.mjs",
      "require": "./dist/versions/3.0.0/calculators.js"
    }
  },
  "typesVersions": {
    "*": {
      "versions/*": [
        "dist/versions/*"
      ],
      "*": [
        "dist/versions/3.0.0/*"
      ]
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "tsup",
    "build:tsc": "tsc",
    "build:tsup": "tsup",
    "check:types": "tsc --noEmit",
    "lint": "pnpm run check:types",
    "test": "jest",
		"version": "npm version --no-git-tag-version"
  },
  "author": "Agricultural Innovation Australia",
  "license": "ISC",
  "description": "",
  "engines": {
    "node": ">=22.19.0"
  },
  "devDependencies": {
    "@swc/core": "^1.13.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "jest": "^30.1.3",
    "nanoclone": "^1.0.2",
    "ts-jest": "^29.4.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "class-validator-jsonschema": "^5.1.0",
    "semver": "^7.7.2",
    "ts-deepmerge": "^7.0.3",
    "type-fest": "^4.41.0"
  }
}
